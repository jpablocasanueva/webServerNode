const http = require('http');

http
  .createServer((req, res) => {
    // console.log(req);

    // res.write('Hola mundo');
    // res.writeHead(200, {
    //   'Content-Type': 'application/json',
    // });

    res.write('Hola Mundo!!');
    res.end();
  })
  .listen(8080);

console.log('Escuchand el puerto: ', 8080);
